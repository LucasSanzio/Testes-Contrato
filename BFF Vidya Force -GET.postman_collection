	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"// 1) Basic Auth\r",
					"(function setupBasicAuth() {\r",
					"  const username = pm.variables.get('username');\r",
					"  const password = pm.variables.get('password');\r",
					"\r",
					"  if (username && password) {\r",
					"    const credentials = btoa(username + ':' + password);\r",
					"    pm.variables.set('auto_token', credentials);\r",
					"  }\r",
					"})();\r",
					"\r",
					"// 2) Sincroniza clientId -> accessData e garante header\r",
					"(function syncAccessData() {\r",
					"  const headerKey = 'accessData';\r",
					"\r",
					"  // Fonte canônica:\r",
					"  // - se o ambiente já tiver clientId (base64), usamos ele;\r",
					"  // - se não, caímos no accessData atual.\r",
					"  let value = pm.variables.get('clientId') || pm.variables.get('accessData') || '';\r",
					"\r",
					"  // Espelha em variável accessData para os scripts de teste\r",
					"  if (value) {\r",
					"    pm.variables.set('accessData', value);\r",
					"  }\r",
					"\r",
					"  // Garante que o header exista e com o valor atualizado\r",
					"  const headers = pm.request.headers;\r",
					"  const existing = headers.get(headerKey);\r",
					"\r",
					"  if (existing !== undefined) {\r",
					"    headers.upsert({ key: headerKey, value });\r",
					"  } else {\r",
					"    headers.add({ key: headerKey, value });\r",
					"  }\r",
					"})();\r",
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"// =======================================================\r",
					"// BACKEND VIDYA FORCE - TESTES DE CONTRATO (v2)\r",
					"// =======================================================\r",
					"\r",
					"// =======================================================\r",
					"// 1. CONTEXTO E HELPERS GERAIS\r",
					"// =======================================================\r",
					"\r",
					"const rawUrl = pm.request.url.toString();\r",
					"const url = rawUrl.toLowerCase();\r",
					"const method = pm.request.method;\r",
					"const status = pm.response.code;\r",
					"const contentType = (pm.response.headers.get(\"Content-Type\") || \"\").toLowerCase();\r",
					"const isJson = contentType.includes(\"application/json\");\r",
					"const requestName = (pm.info.requestName || \"\").toLowerCase();\r",
					"\r",
					"// Tenta parsear JSON (sem quebrar se não for JSON)\r",
					"let json = null;\r",
					"let jsonParseError = false;\r",
					"if (isJson) {\r",
					"    try {\r",
					"        json = pm.response.json();\r",
					"    } catch (e) {\r",
					"        jsonParseError = true;\r",
					"    }\r",
					"}\r",
					"\r",
					"\r",
					"// Flags gerais de envelope de erro/sucesso (apenas em memória, não viram variáveis do Postman)\r",
					"const hasErrorFlag =\r",
					"  isJson &&\r",
					"  json &&\r",
					"  Object.prototype.hasOwnProperty.call(json, \"hasError\") &&\r",
					"  json.hasError === true;\r",
					"\r",
					"const isSuccessEnvelope =\r",
					"  isJson &&\r",
					"  json &&\r",
					"  Object.prototype.hasOwnProperty.call(json, \"hasError\") &&\r",
					"  json.hasError === false;\r",
					"\r",
					"// Identifica se é request NEGATIVO pelo nome\r",
					"const isNegativeCase =\r",
					"    requestName.includes(\"[negativo]\") ||\r",
					"    requestName.includes(\"[error]\") ||\r",
					"    requestName.includes(\"[erro]\") ||\r",
					"    requestName.includes(\"[4xx]\") ||\r",
					"    requestName.includes(\"[5xx]\");\r",
					"\r",
					"// Segmentar path em minúsculas\r",
					"const pathSegments = (pm.request.url.path || [])\r",
					"    .filter(Boolean)\r",
					"    .map(s => String(s).toLowerCase());\r",
					"\r",
					"// Resolve chave de módulo automática\r",
					"function getModuleKey() {\r",
					"    if (!pathSegments.length) return \"root\";\r",
					"    if (pathSegments[0] === \"ppid\") {\r",
					"        if (pathSegments.length > 1) {\r",
					"            return \"ppid_\" + pathSegments[1];\r",
					"        }\r",
					"        return \"ppid_root\";\r",
					"    }\r",
					"    return pathSegments[0];\r",
					"}\r",
					"\r",
					"const moduleKey = getModuleKey();\r",
					"\r",
					"// =======================================================\r",
					"// 1.5 SMOKE TEST + GATE (Status 2xx)\r",
					"// =======================================================\r",
					"\r",
					"// Só executa o Gate se não for um cenário negativo intencional\r",
					"if (!isNegativeCase) {\r",
					"    let smokeFailed = false;\r",
					"\r",
					"    // Status deve ser 2xx (200 a 299)\r",
					"    if (status < 200 || status >= 300) {\r",
					"        smokeFailed = true;\r",
					"    }\r",
					"\r",
					"    // Testa o status e falha se não for 2xx\r",
					"    pm.test(`[SMOKE] Status 2xx esperado`, () => {\r",
					"        pm.expect(status, `Status inesperado: ${status} (${rawUrl})`).to.be.within(200, 299);\r",
					"    });\r",
					"\r",
					"    // Se o smoke falhou, interrompe o script\r",
					"    if (smokeFailed) {\r",
					"        console.log(`[GATE] Smoke falhou (Status ${status}). Ignorando testes avançados para ${rawUrl}.`);\r",
					"        return; // <-- Interrompe a execução dos testes subsequentes\r",
					"    }\r",
					"    // Interrompe se hasError=true mesmo com status 2xx (Simplificação aplicada)\r",
					"    if (hasErrorFlag) {\r",
					"        pm.test(\"[GATE] Resposta com hasError=true deve falhar\", () => {\r",
					"            pm.expect.fail(`hasError=true detectado em resposta com status ${status} (${rawUrl})`);\r",
					"        });\r",
					"        console.log(`[GATE] hasError=true detectado. Interrompendo testes para ${rawUrl}.`);\r",
					"        return; // <-- Interrompe execução dos testes subsequentes\r",
					"    }\r",
					"\r",
					"}\r",
					"\r",
					"// Helpers para respostas no padrão BaseList\r",
					"function isBaseListResponse(body) {\r",
					"    if (!body || typeof body !== \"object\") return false;\r",
					"    const hasHasError = Object.prototype.hasOwnProperty.call(body, \"hasError\");\r",
					"    // Refatoração: Normaliza a chave para verificar qtdRegistros\r",
					"    const qtdKey = Object.keys(body).find(k => k.toLowerCase() === \"qtdregistros\");\r",
					"    const hasQtd = !!qtdKey;\r",
					"    const hasData = Array.isArray(body.data);\r",
					"    return hasHasError && hasQtd && hasData;\r",
					"}\r",
					"\r",
					"function getMainArray(body) {\r",
					"    if (!body) return [];\r",
					"    if (Array.isArray(body)) return body;\r",
					"    if (Array.isArray(body.data)) return body.data;\r",
					"    return [];\r",
					"}\r",
					"\r",
					"// Helper genérico: pelo menos 1 chave presente\r",
					"function ensureAtLeastOneKey(obj, keys, msg) {\r",
					"    const ok = keys.some(k => Object.prototype.hasOwnProperty.call(obj, k));\r",
					"    pm.expect(ok, msg || `Deve possuir pelo menos um dos campos: ${keys.join(\", \")}`).to.be.true;\r",
					"}\r",
					"\r",
					"// Helper: tipo esperado\r",
					"function ensureFieldType(value, expectedTypes, msg) {\r",
					"    const types = Array.isArray(expectedTypes) ? expectedTypes : [expectedTypes];\r",
					"    const actual = typeof value;\r",
					"    const ok = types.includes(actual);\r",
					"    pm.expect(ok, msg || `Tipo ${actual} não está entre os esperados: ${types.join(\", \")}`).to.be.true;\r",
					"}\r",
					"\r",
					"\r",
					"// =======================================================\r",
					"// 2. CONTRATO GERAL DE JSON\r",
					"// =======================================================\r",
					"\r",
					"// 2.1 JSON válido quando Content-Type indica application/json\r",
					"pm.test(\"[CT-001] [CONTRACT][GENERIC] JSON válido quando Content-Type é application/json\", () => {\r",
					"    if (!isJson) {\r",
					"        return pm.expect(true, \"N/A\").to.be.true;\r",
					"    }\r",
					"    pm.expect(jsonParseError, \"Falha ao parsear JSON da resposta\").to.be.false;\r",
					"});\r",
					"\r",
					"// O restante dos testes genéricos só roda se for JSON válido\r",
					"if (isJson && json && typeof json === \"object\") {\r",
					"    const bodyStr = JSON.stringify(json).toLowerCase();\r",
					"\r",
					"    // 2.2 Resposta JSON não contém HTML bruto\r",
					"    pm.test(\"[CT-001] [CONTRACT][GENERIC] Resposta JSON não contém HTML\", () => {\r",
					"        pm.expect(bodyStr).to.not.include(\"<html\");\r",
					"    });\r",
					"\r",
					"    // 2.3 Convenção hasError\r",
					"    if (Object.prototype.hasOwnProperty.call(json, \"hasError\")) {\r",
					"        pm.test(\"[CT-001] [CONTRACT][GENERIC] hasError é booleano\", () => {\r",
					"            pm.expect(json.hasError, \"hasError deve ser booleano\").to.be.a(\"boolean\");\r",
					"        });\r",
					"\r",
					"        pm.test(\"[CT-001] [CONTRACT][GENERIC] Estrutura de erro quando hasError = true\", () => {\r",
					"            if (json.hasError === true) {\r",
					"                const hasMsg =\r",
					"                    json.message ||\r",
					"                    json.mensagem ||\r",
					"                    json.error ||\r",
					"                    (Array.isArray(json.errors) && json.errors.length > 0);\r",
					"                pm.expect(!!hasMsg, \"hasError=true sem mensagem/erro detalhado\").to.be.true;\r",
					"            }\r",
					"        });\r",
					"\r",
					"        pm.test(\"[CT-001] [CONTRACT][GENERIC] Sucesso não vaza stack/exception\", () => {\r",
					"            if (json.hasError === false) {\r",
					"                const lixo =\r",
					"                    json.stackTrace ||\r",
					"                    json.exception ||\r",
					"                    json.developerMessage ||\r",
					"                    json.error;\r",
					"                pm.expect(!!lixo, \"Campos de erro vazando em sucesso\").to.be.false;\r",
					"            }\r",
					"        });\r",
					"    }\r",
					"}\r",
					"\r",
					"\r",
					"// =======================================================\r",
					"// 3. CONTRATO PARA ENVELOPES BASELIST\r",
					"// =======================================================\r",
					"\r",
					"// O teste de BaseList não deve rodar em cenários negativos, a menos que seja um BaseList de erro\r",
					"if (isJson && json && isBaseListResponse(json) && !isNegativeCase) {\r",
					"    const data = getMainArray(json);\r",
					"\r",
					"    pm.test(\"[CT-002] [CONTRACT][BaseList] Estrutura mínima válida\", () => {\r",
					"        pm.expect(json).to.have.property(\"hasError\");\r",
					"        // Busca a chave de qtdRegistros de forma case-insensitive\r",
					"        const qtdKey = Object.keys(json).find(k => k.toLowerCase() === \"qtdregistros\");\r",
					"        pm.expect(json).to.have.property(qtdKey);\r",
					"        pm.expect(json).to.have.property(\"data\").that.is.an(\"array\");\r",
					"        pm.expect(json.hasError, \"hasError deve ser booleano\").to.be.a(\"boolean\");\r",
					"        \r",
					"        const qtd = json[qtdKey];\r",
					"        const tipoQtdOk =\r",
					"            typeof qtd === \"number\" ||\r",
					"            typeof qtd === \"string\";\r",
					"        pm.expect(tipoQtdOk, \"qtdRegistros deve ser number ou string numérica\").to.be.true;\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-002] [CONTRACT][BaseList] Coerência entre qtdRegistros e data.length\", () => {\r",
					"        const qtdKey = Object.keys(json).find(k => k.toLowerCase() === \"qtdregistros\");\r",
					"        const qtd = Number(json[qtdKey]);\r",
					"        if (!Number.isNaN(qtd)) {\r",
					"            pm.expect(qtd, \"qtdRegistros divergente de data.length\")\r",
					"              .to.eql(data.length);\r",
					"        }\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-002] [CONTRACT][BaseList] Se qtdRegistros > 0 então data não é vazia\", () => {\r",
					"        const qtdKey = Object.keys(json).find(k => k.toLowerCase() === \"qtdregistros\");\r",
					"        const qtd = Number(json[qtdKey]);\r",
					"        if (!Number.isNaN(qtd) && qtd > 0) {\r",
					"            pm.expect(data.length, \"qtdRegistros > 0 mas data está vazia\").to.be.above(0);\r",
					"        }\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-002] [CONTRACT][BaseList] Itens são objetos\", () => {\r",
					"        data.forEach((item, i) => {\r",
					"            pm.expect(item, `Item[${i}] não é objeto`).to.be.an(\"object\");\r",
					"        });\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-002] [CONTRACT][BaseList] Paginação consistente (se presente)\", () => {\r",
					"        if (json.page !== undefined) {\r",
					"            pm.expect(json.page, \"page deve ser numérico\").to.be.a(\"number\");\r",
					"        }\r",
					"        if (json.pageSize !== undefined) {\r",
					"            pm.expect(json.pageSize, \"pageSize deve ser numérico\").to.be.a(\"number\");\r",
					"        }\r",
					"        if (json.totalPages !== undefined) {\r",
					"            pm.expect(json.totalPages, \"totalPages deve ser numérico\").to.be.a(\"number\");\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"\r",
					"// =======================================================\r",
					"// 4. CONTRATOS POR MÓDULO / ENDPOINT\r",
					"// (Conteúdo de CT-003 a CT-015 mantido inalterado)\r",
					"// =======================================================\r",
					"\r",
					"// 4.1 AUTENTICAÇÃO / LOGIN (Login + newLogin)\r",
					"if (isJson && json && (moduleKey === \"ppid_login\" || url.includes(\"/ppid/newlogin\")) && !isNegativeCase) {\r",
					"    pm.test(\"[CT-003] [CONTRACT][LOGIN] Envelope padrão com hasError\", () => {\r",
					"        pm.expect(json).to.have.property(\"hasError\");\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-003] [CONTRACT][LOGIN] Sucesso contém dados mínimos de sessão\", () => {\r",
					"        if (json.hasError === false && String(status)[0] === \"2\") {\r",
					"            const hasAuthData =\r",
					"                json.token ||\r",
					"                json.auth ||\r",
					"                json.accessToken ||\r",
					"                json.bearer ||\r",
					"                json.usuario ||\r",
					"                json.user;\r",
					"            pm.expect(!!hasAuthData, \"Login sem token/usuário/autorização no sucesso\").to.be.true;\r",
					"\r",
					"            if (json.token) {\r",
					"                ensureFieldType(json.token, \"string\", \"Token de autenticação deve ser string\");\r",
					"            }\r",
					"            if (json.expiraEm || json.expiresIn) {\r",
					"                ensureFieldType(json.expiraEm || json.expiresIn, [\"number\", \"string\"], \"Tempo de expiração inválido\");\r",
					"            }\r",
					"        }\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-003] [CONTRACT][LOGIN] Erro de login com mensagem clara\", () => {\r",
					"        if (json.hasError === true || status >= 400) {\r",
					"            const hasMsg =\r",
					"                json.message ||\r",
					"                json.mensagem ||\r",
					"                json.error ||\r",
					"                (Array.isArray(json.errors) && json.errors.length > 0);\r",
					"            pm.expect(!!hasMsg, \"Erro de login sem mensagem detalhada\").to.be.true;\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.2 PRODUTOS (getPrices, list, etc.)\r",
					"if (isJson && json && (moduleKey === \"ppid_getprices\" || moduleKey === \"produto\") && !isNegativeCase && !hasErrorFlag) {\r",
					"    const data = getMainArray(json);\r",
					"\r",
					"    pm.test(\"[CT-004] [CONTRACT][PRODUTO] Estrutura mínima de lista\", () => {\r",
					"        if (isBaseListResponse(json)) {\r",
					"            pm.expect(data.length).to.be.at.least(0);\r",
					"        }\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-005] [CONTRACT][PRODUTO] Campos-chave por produto\", () => {\r",
					"        if (Array.isArray(data) && data.length > 0) {\r",
					"            data.forEach((p, i) => {\r",
					"                ensureAtLeastOneKey(\r",
					"                    p,\r",
					"                    [\"codProd\", \"codprod\", \"id\", \"sku\"],\r",
					"                    `[PRODUTO] Item[${i}] sem identificador (codProd/id/sku)`\r",
					"                );\r",
					"                ensureAtLeastOneKey(\r",
					"                    p,\r",
					"                    [\"nome\", \"descricao\", \"description\"],\r",
					"                    `[PRODUTO] Item[${i}] sem nome/descrição`\r",
					"                );\r",
					"                if (p.preco !== undefined) {\r",
					"                    ensureFieldType(p.preco, [\"number\", \"string\"], `[PRODUTO] Item[${i}] preço inválido`);\r",
					"                    // Adição: Preço deve ser positivo (ou zero)\r",
					"                    if (typeof p.preco === 'number') {\r",
					"                        pm.expect(p.preco, `[PRODUTO] Item[${i}] preço negativo`).to.be.at.least(0);\r",
					"                    }\r",
					"                }\r",
					"            });\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.3 PEDIDOS (list, get, etc.)\r",
					"if (isJson && json && moduleKey === \"pedido\" && !isNegativeCase && !hasErrorFlag) {\r",
					"    const data = getMainArray(json);\r",
					"\r",
					"    pm.test(\"[CT-006] [CONTRACT][PEDIDO] Estrutura mínima de lista\", () => {\r",
					"        if (isBaseListResponse(json)) {\r",
					"            pm.expect(data.length).to.be.at.least(0);\r",
					"        }\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-007] [CONTRACT][PEDIDO] Campos-chave por pedido\", () => {\r",
					"        if (Array.isArray(data) && data.length > 0) {\r",
					"            data.forEach((p, i) => {\r",
					"                ensureAtLeastOneKey(\r",
					"                    p,\r",
					"                    [\"codPed\", \"codped\", \"id\"],\r",
					"                    `[PEDIDO] Item[${i}] sem identificador (codPed/id)`\r",
					"                );\r",
					"                ensureAtLeastOneKey(\r",
					"                    p,\r",
					"                    [\"data\", \"dataCriacao\", \"dataEmissao\"],\r",
					"                    `[PEDIDO] Item[${i}] sem data de criação`\r",
					"                );\r",
					"            });\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.4 CLIENTES (list, get, etc.)\r",
					"if (isJson && json && moduleKey === \"cliente\" && !isNegativeCase && !hasErrorFlag) {\r",
					"    const data = getMainArray(json);\r",
					"\r",
					"    pm.test(\"[CT-008] [CONTRACT][CLIENTE] Estrutura mínima de lista\", () => {\r",
					"        if (isBaseListResponse(json)) {\r",
					"            pm.expect(data.length).to.be.at.least(0);\r",
					"        }\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-009] [CONTRACT][CLIENTE] Campos-chave por cliente\", () => {\r",
					"        if (Array.isArray(data) && data.length > 0) {\r",
					"            data.forEach((c, i) => {\r",
					"                ensureAtLeastOneKey(\r",
					"                    c,\r",
					"                    [\"codCli\", \"codcli\", \"id\"],\r",
					"                    `[CLIENTE] Item[${i}] sem identificador (codCli/id)`\r",
					"                );\r",
					"                ensureAtLeastOneKey(\r",
					"                    c,\r",
					"                    [\"nome\", \"razaoSocial\"],\r",
					"                    `[CLIENTE] Item[${i}] sem nome/razão social`\r",
					"                );\r",
					"            });\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.5 ENDEREÇOS (list, get, etc.)\r",
					"if (isJson && json && moduleKey === \"endereco\" && !isNegativeCase && !hasErrorFlag) {\r",
					"    const data = getMainArray(json);\r",
					"\r",
					"    pm.test(\"[CT-010] [CONTRACT][ENDERECO] Campos-chave por endereço\", () => {\r",
					"        if (Array.isArray(data) && data.length > 0) {\r",
					"            data.forEach((e, i) => {\r",
					"                ensureAtLeastOneKey(\r",
					"                    e,\r",
					"                    [\"cep\", \"logradouro\", \"cidade\"],\r",
					"                    `[ENDERECO] Item[${i}] sem campos-chave (cep/logradouro/cidade)`\r",
					"                );\r",
					"            });\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.6 PARCEIROS (list, get, etc.)\r",
					"if (isJson && json && moduleKey === \"partner\" && !isNegativeCase && !hasErrorFlag) {\r",
					"    const data = getMainArray(json);\r",
					"\r",
					"    pm.test(\"[CT-011] [CONTRACT][PARCEIROS] Estrutura mínima de lista\", () => {\r",
					"        if (isBaseListResponse(json)) {\r",
					"            pm.expect(data.length).to.be.at.least(0);\r",
					"        }\r",
					"    });\r",
					"\r",
					"    pm.test(\"[CT-011] [CONTRACT][PARCEIROS] Campos-chave por parceiro\", () => {\r",
					"        if (Array.isArray(data) && data.length > 0) {\r",
					"            data.forEach((p, i) => {\r",
					"                ensureAtLeastOneKey(\r",
					"                    p,\r",
					"                    [\"codParc\", \"CODPARC\"],\r",
					"                    `[PARCEIROS] Item[${i}] sem codParc`\r",
					"                );\r",
					"                if (p.CGC_CPF || p.CNPJ || p.cnpj || p.cpf) {\r",
					"                    const doc = (p.CGC_CPF || p.CNPJ || p.cnpj || p.cpf || \"\").toString().replace(/\\D/g, \"\");\r",
					"                    if (doc) {\r",
					"                        pm.expect([11, 14], `[PARCEIROS] Item[${i}] documento com tamanho inválido`).to.include(doc.length);\r",
					"                    }\r",
					"                }\r",
					"            });\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.7 USUÁRIOS / VENDEDORES\r",
					"if (\r",
					"  isJson &&\r",
					"  json &&\r",
					"  moduleKey === \"user\" &&\r",
					"  !isNegativeCase &&\r",
					"  !hasErrorFlag &&\r",
					"  !url.includes(\"/versaominima\")\r",
					") {\r",
					"    pm.test(\"[CT-012] [CONTRACT][USUARIO] Estrutura mínima\", () => {\r",
					"        const data = Array.isArray(json) ? json : getMainArray(json);\r",
					"        const arr = Array.isArray(data) && data.length ? data : [json];\r",
					"\r",
					"        arr.forEach((u, i) => {\r",
					"            if (!u || typeof u !== \"object\") return;\r",
					"            ensureAtLeastOneKey(\r",
					"                u,\r",
					"                [\"nome\", \"name\", \"usuario\", \"login\"],\r",
					"                `[USUARIO] Registro[${i}] sem identificação`\r",
					"            );\r",
					"        });\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.8 CONFIGURAÇÕES / VERSÃO MÍNIMA\r",
					"if (isJson && json && url.includes(\"/user/versaominima\") && !isNegativeCase) {\r",
					"    pm.test(\"[CT-013] [CONTRACT][CONFIG] versaoMinima presente\", () => {\r",
					"        pm.expect(json).to.have.property(\"versaoMinima\");\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.9 LOGÍSTICA / FRETE / FERIADOS (quando JSON)\r",
					"if (isJson && json && (\r",
					"    url.includes(\"/tabelafrete\") ||\r",
					"    url.includes(\"/regrasentregas\") ||\r",
					"    url.includes(\"/feriados\")\r",
					") && !isNegativeCase && !hasErrorFlag) {\r",
					"    pm.test(\"[CT-014] [CONTRACT][LOGISTICA] Estrutura válida\", () => {\r",
					"        if (!isBaseListResponse(json)) {\r",
					"            pm.expect(\r",
					"                Array.isArray(json) || Array.isArray(json.data) || typeof json === \"object\",\r",
					"                \"[LOGISTICA] Estrutura inesperada\"\r",
					"            ).to.be.true;\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"// 4.10 DOCUMENTOS (viewDanfe, viewBoleto, viewPdf) - quando retornam JSON de erro\r",
					"if (isJson && json && (\r",
					"    url.includes(\"viewdanfe\") ||\r",
					"    url.includes(\"viewboleto\") ||\r",
					"    url.includes(\"viewpdf\")\r",
					")) {\r",
					"    // Este teste é mantido mesmo em cenários negativos, pois valida o contrato de erro\r",
					"    pm.test(\"[CT-015] [CONTRACT][DOCS] Erros padronizados em consultas de documentos\", () => {\r",
					"        if (status >= 400 || json.hasError === true) {\r",
					"            const msg =\r",
					"                json.message ||\r",
					"                json.mensagem ||\r",
					"                json.error ||\r",
					"                json.errors;\r",
					"            pm.expect(!!msg, \"[DOCS] Erro em documento sem mensagem\").to.be.true;\r",
					"        }\r",
					"    });\r",
					"}\r",
					"\r",
					"\r",
					"// =======================================================\r",
					"// 5. CONTRATOS PARA RESPOSTAS DE ERRO (4xx/5xx)\r",
					"// =======================================================\r",
					"\r",
					"// 5.1 Erros JSON\r",
					"if (isJson && json && status >= 400) {\r",
					"    pm.test(\"[CT-001] [CONTRACT][ERROR] Estrutura mínima de erro em respostas 4xx/5xx\", () => {\r",
					"        const hasMensagem =\r",
					"            json.hasError === true ||\r",
					"            json.message ||\r",
					"            json.mensagem ||\r",
					"            json.error ||\r",
					"            json.errors;\r",
					"        pm.expect(\r",
					"            hasMensagem,\r",
					"            \"Erro sem indicação clara (hasError/message/mensagem/error/errors)\"\r",
					"        ).to.exist;\r",
					"    });\r",
					"    \r",
					"    // Adição: hasError deve ser true em erros 4xx/5xx que retornam JSON\r",
					"    if (Object.prototype.hasOwnProperty.call(json, \"hasError\")) {\r",
					"        pm.test(\"[CT-001] [CONTRACT][ERROR] hasError deve ser TRUE em 4xx/5xx\", () => {\r",
					"            pm.expect(json.hasError, \"hasError deve ser true em respostas de erro 4xx/5xx\").to.be.true;\r",
					"        });\r",
					"    }\r",
					"}\r",
					"\r",
					"// 5.2 Erros Não-JSON\r",
					"if (!isJson && status >= 400) {\r",
					"    pm.test(\"[CT-001] [CONTRACT][ERROR] Resposta de erro não-JSON não deve ser vazia\", () => {\r",
					"        pm.expect(pm.response.text().length, \"Corpo da resposta de erro não-JSON está vazio\").to.be.above(0);\r",
					"    });\r",
					"    \r",
					"    pm.test(\"[CT-001] [CONTRACT][ERROR] Resposta de erro não-JSON não deve conter HTML (stack trace)\", () => {\r",
					"        const bodyStr = pm.response.text().toLowerCase();\r",
					"        pm.expect(bodyStr).to.not.include(\"<html\");\r",
					"        pm.expect(bodyStr).to.not.include(\"stack trace\");\r",
					"    });\r",
					"}\r",
					"\r",
					"// =======================================================\r",
					"// 7. ADD-ON V3 - CONTRATOS BINÁRIOS E PAGINAÇÃO\r",
					"// =======================================================\r",
					"\r",
					"(function V3_ADDON_CONTRACTS() {\r",
					"  // Recria o contexto local para usar as variáveis do escopo global do Postman\r",
					"  const req = pm.request;\r",
					"  const res = pm.response;\r",
					"  const ct = contentType;\r",
					"  const u = url;\r",
					"\r",
					"  // A) BINÁRIOS (PDF / DANFE / BOLETO / IMAGENS)\r",
					"  (function binaryChecks() {\r",
					"    // PDF-like\r",
					"    if ((res.code >= 200 && res.code < 300) && !isJson && (u.includes('/viewpdf') || u.includes('/viewdanfe') || u.includes('/viewboleto'))) {\r",
					"      pm.test('[CT-017] [BINARIO] Content-Type PDF', () => pm.expect(ct).to.include('application/pdf'));\r",
					"      pm.test('[CT-017] [BINARIO] Tamanho > 1KB', () => pm.expect(res.responseSize).to.be.above(1024));\r",
					"      pm.test('[CT-017] [BINARIO] Content-Disposition presente', () => {\r",
					"        const cd = res.headers.get('Content-Disposition') || '';\r",
					"        pm.expect(cd.length > 0, 'Content-Disposition ausente').to.be.true;\r",
					"      });\r",
					"    }\r",
					"\r",
					"    // Imagens (produto/usuário)\r",
					"    if ((res.code >= 200 && res.code < 300) && !isJson && u.includes('/imagem/')) {\r",
					"      pm.test('[CT-017] [BINARIO] Content-Type imagem', () =>\r",
					"        pm.expect(ct).to.match(/image\\/(png|jpe?g|webp)/));\r",
					"      pm.test('[CT-017] [BINARIO] Tamanho > 512B', () => pm.expect(res.responseSize).to.be.above(512));\r",
					"    }\r",
					"  })();\r",
					"\r",
					"  // B) PAGINAÇÃO (Coerência de página)\r",
					"  (function paginationChecks() {\r",
					"    if (!isJson || !url.includes('/ppid/getprices') || isNegativeCase) return;\r",
					"\r",
					"    // Coerência com a query (mantido por ser um teste de contrato de dados)\r",
					"    if (json && json.page !== undefined) {\r",
					"        const q = req.url.query || [];\r",
					"        const qPage = q.find(x => x.key === 'page');\r",
					"        const page = qPage ? Number(qPage.value) : undefined;\r",
					"\r",
					"        if (page !== undefined) {\r",
					"          pm.test('[CT-018] [PAG] \"page\" coerente entre query e resposta', () => {\r",
					"            pm.expect(Number(json.page)).to.eql(Number(page));\r",
					"          });\r",
					"        }\r",
					"    }\r",
					"  })();\r",
					"})();\r",
					""
				]
			}
		}
	],
	"variable": [
		{
			"key": "baseUrl",
			"value": "http://localhost:8080"

			],
			"description": "Endpoints from 14. Other Integration controllers"
		}
	],
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"// Script pré-requisição global\r",
					"// === Auto-injeção de codVend em endpoints de partner/list* ===\r",
					"(function addCodVendToPartnerLists() {\r",
					"  try {\r",
					"    const url = pm.request.url;\r",
					"    const path = url.getPath(); // ex: \"/partner/listByName\"\r",
					"\r",
					"    // Só mexe em rotas de partner com \"list\" no final do path\r",
					"    const isPartner = path.startsWith(\"/partner/\");\r",
					"    const isListLike = path.toLowerCase().includes(\"list\");\r",
					"\r",
					"    if (!isPartner || !isListLike) {\r",
					"      return; // não é alvo, sai fora\r",
					"    }\r",
					"\r",
					"    // Já tem codVend na query? então não faz nada\r",
					"    const hasCodVend =\r",
					"      url.query &&\r",
					"      url.query.some(q => q && q.key === \"codVend\");\r",
					"\r",
					"    if (hasCodVend) {\r",
					"      return;\r",
					"    }\r",
					"\r",
					"    // Pega codVend de variável (collection ou environment)\r",
					"    const codVend =\r",
					"      pm.collectionVariables.get(\"codVend\") ||\r",
					"      pm.environment.get(\"codVend\");\r",
					"\r",
					"    if (!codVend) {\r",
					"      // nada configurado, não adiciona pra não quebrar\r",
					"      console.warn(\"[AUTO-codVend] Variável codVend não configurada.\");\r",
					"      return;\r",
					"    }\r",
					"\r",
					"    url.addQueryParams({ key: \"codVend\", value: String(codVend) });\r",
					"  } catch (e) {\r",
					"    console.warn(\"[AUTO-codVend] Erro ao injetar codVend:\", e);\r",
					"  }\r",
					"})();\r",
					"\r",
					"// 1) Basic Auth\r",
					"(function setupBasicAuth() {\r",
					"  const username = pm.variables.get('username');\r",
					"  const password = pm.variables.get('password');\r",
					"\r",
					"  if (username && password) {\r",
					"    const credentials = btoa(username + ':' + password);\r",
					"    pm.variables.set('auto_token', credentials);\r",
					"  }\r",
					"})();\r",
					"\r",
					"// 2) Sincroniza clientId -> accessData e garante header\r",
					"(function syncAccessData() {\r",
					"  const headerKey = 'accessData';\r",
					"\r",
					"  // Fonte canônica:\r",
					"  // - se o ambiente já tiver clientId (base64), usamos ele;\r",
					"  // - se não, caímos no accessData atual.\r",
					"  let value = pm.variables.get('clientId') || pm.variables.get('accessData') || '';\r",
					"\r",
					"  // Espelha em variável accessData para os scripts de teste\r",
					"  if (value) {\r",
					"    pm.variables.set('accessData', value);\r",
					"  }\r",
					"\r",
					"  // Garante que o header exista e com o valor atualizado\r",
					"  const headers = pm.request.headers;\r",
					"  const existing = headers.get(headerKey);\r",
					"\r",
					"  if (existing !== undefined) {\r",
					"    headers.upsert({ key: headerKey, value });\r",
					"  } else {\r",
					"    headers.add({ key: headerKey, value });\r",
					"  }\r",
					"})();\r",
					""
				]
			}
		},

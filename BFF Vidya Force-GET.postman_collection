					"let jsonParseError = false; // Adicionado do script do usuário\r",
					"if (isJson) {\r",
					"    try {\r",
					"        json = pm.response.json();\r",
					"    } catch (e) {\r",
					"        jsonParseError = true; // Adicionado do script do usuário\r",
					"    }\r",
					"}\r",
					"\r",
					"\r",
					"\r",
					"// Flags gerais de envelope de erro/sucesso (apenas em memória, não viram variáveis do Postman)\r",
					"const hasErrorFlag =\r",
					"  isJson &&\r",
					"  json &&\r",
					"  Object.prototype.hasOwnProperty.call(json, \"hasError\") &&\r",
					"  json.hasError === true;\r",
					"\r",
					"const isSuccessEnvelope =\r",
					"  isJson &&\r",

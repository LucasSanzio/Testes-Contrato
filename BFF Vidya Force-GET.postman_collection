					"    });\r",
					"\r",
					"    // Se o smoke falhou, interrompe o script\r",
					"    if (smokeFailed) {\r",
					"        console.log(`[GATE] Smoke falhou (Status ${status}). Ignorando testes avançados para ${rawUrl}.`);\r",
					"        return; // <-- Interrompe a execução dos testes subsequentes\r",
					"    }\r",
					"    // Interrompe se hasError=true mesmo com status 2xx\r",
					"    if (isSuccessEnvelope === false && hasErrorFlag === true) {\r",
					"        pm.test(\"[GATE] Resposta com hasError=true deve falhar\", () => {\r",
					"            pm.expect.fail(`hasError=true detectado em resposta com status ${status} (${rawUrl})`);\r",
					"        });\r",
					"        console.log(`[GATE] hasError=true detectado. Interrompendo testes para ${rawUrl}.`);\r",
					"        return; // <-- Interrompe execução dos testes subsequentes\r",
					"    }\r",
					"\r",

					"\r",
					"// =======================================================\r",
					"// 1. CONTEXTO E HELPERS GERAIS\r",
					"// =======================================================\r",
					"\r",
					"const rawUrl = pm.request.url.toString();\r",
					"const url = rawUrl.toLowerCase();\r",
					"const method = pm.request.method;\r",
					"const status = pm.response.code;\r",
					"const contentType = (pm.response.headers.get(\"Content-Type\") || \"\").toLowerCase();\r",

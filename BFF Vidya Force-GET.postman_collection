					"    const hasHasError = Object.prototype.hasOwnProperty.call(body, \"hasError\");\r",
					"    const hasQtd =\r",
					"        Object.prototype.hasOwnProperty.call(body, \"qtdRegistros\") ||\r",
					"        Object.prototype.hasOwnProperty.call(body, \"qtdregistros\");\r",

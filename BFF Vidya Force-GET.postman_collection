					"// Helper genérico: pelo menos 1 chave presente\r",
					"function ensureAtLeastOneKey(obj, keys, msg) {\r",
					"    const ok = keys.some(k => Object.prototype.hasOwnProperty.call(obj, k));\r",
					"    pm.expect(ok, msg || `Deve possuir pelo menos um dos campos: ${keys.join(\", \")}`).to.be.true;\r",
					"}\r",
					"\r",
					"// Helper: tipo esperado\r",
					"function ensureFieldType(value, expectedTypes, msg) {\r",
					"    const types = Array.isArray(expectedTypes) ? expectedTypes : [expectedTypes];\r",
					"    const actual = typeof value;\r",
					"    const ok = types.includes(actual);\r",
					"    pm.expect(ok, msg || `Tipo ${actual} não está entre os esperados: ${types.join(\", \")}`).to.be.true;\r",

					"    requestName.includes(\"[erro]\") ||\r",
					"    requestName.includes(\"[4xx]\") ||\r",
					"    requestName.includes(\"[5xx]\");\r",
					"\r",
					"// Segmentar path em minúsculas\r",
					"const pathSegments = (pm.request.url.path || [])\r",
					"    .filter(Boolean)\r",
					"    .map(s => String(s).toLowerCase());\r",
					"\r",
					"// Resolve chave de módulo automática\r",
					"function getModuleKey() {\r",
					"    if (!pathSegments.length) return \"root\";\r",
					"    if (pathSegments[0] === \"ppid\") {\r",
					"        if (pathSegments.length > 1) {\r",
					"            return \"ppid_\" + pathSegments[1];\r",
					"        }\r",
					"        return \"ppid_root\";\r",
					"    }\r",
					"    return pathSegments[0];\r",
					"}\r",
					"\r",
					"const moduleKey = getModuleKey();\r",
					"\r",
					"// =======================================================\r",
					"// 1.5 SMOKE TEST + GATE (Status 2xx)\r",
					"// =======================================================\r",
					"\r",
					"// Só executa o Gate se não for um cenário negativo intencional\r",
					"if (!isNegativeCase) {\r",
					"    let smokeFailed = false;\r",
					"\r",
					"    // Status deve ser 2xx (200 a 299)\r",
					"    if (status < 200 || status >= 300) {\r",
					"        smokeFailed = true;\r",
					"    }\r",
					"\r",
					"    // Testa o status e falha se não for 2xx\r",
